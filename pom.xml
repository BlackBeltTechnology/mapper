    <properties>
        <project-shortname>mapper</project-shortname>
        <project-repositoryId>BlackBeltTechnology/mapper</project-repositoryId>

        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.deploy.skip>false</maven.deploy.skip>
        <java-version>1.8</java-version>

        <slf4j-api-version>1.6.0</slf4j-api-version>
        <slf4j-version>1.7.26</slf4j-version>
        <lombok-version>1.18.8</lombok-version>

        <osgi-version>6.0.0</osgi-version>
        <osgi-ds-version>1.3.0</osgi-ds-version>

        <guava-version>20.0</guava-version>

        <jupiter-version>5.5.1</jupiter-version>
        <mockito-version>3.0.0</mockito-version>
        <hamcrest-version>2.1</hamcrest-version>

        <checkstyle-version>8.22</checkstyle-version>
        <checkstyle-plugin-version>3.1.0</checkstyle-plugin-version>
        <checkstyle-skip>false</checkstyle-skip>
        <pmd-version>6.17.0</pmd-version>
        <pmd-plugin-version>3.12.0</pmd-plugin-version>
        <findbugs-version>3.0.1</findbugs-version>
        <findbugs-plugin-version>3.0.5</findbugs-plugin-version>
        <findbugs-annotations-version>3.0.1</findbugs-annotations-version>
        <checkstyle-skip>false</checkstyle-skip>
        <pmd-skip>false</pmd-skip>
        <findbugs-skip>false</findbugs-skip>
        <qa-failOnError>true</qa-failOnError>

        <joda-time-version>2.10.3</joda-time-version>
        <pax-exam-version>4.13.1</pax-exam-version>
        <karaf-version>4.2.6</karaf-version>

        <log4j.configuration>log4j.properties</log4j.configuration>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>org.slf4j</groupId>
                <artifactId>slf4j-api</artifactId>
                <version>${slf4j-api-version}</version>
                <scope>provided</scope>
            </dependency>

            <dependency>
                <groupId>org.projectlombok</groupId>
                <artifactId>lombok</artifactId>
                <version>${lombok-version}</version>
                <scope>provided</scope>
            </dependency>

            <dependency>
                <groupId>org.osgi</groupId>
                <artifactId>osgi.core</artifactId>
                <version>${osgi-version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>org.osgi</groupId>
                <artifactId>osgi.cmpn</artifactId>
                <version>${osgi-version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>org.osgi</groupId>
                <artifactId>org.osgi.annotation</artifactId>
                <version>${osgi-version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>org.osgi</groupId>
                <artifactId>org.osgi.service.component.annotations</artifactId>
                <version>${osgi-ds-version}</version>
                <scope>provided</scope>
            </dependency>
            <dependency>
                <groupId>org.osgi</groupId>
                <artifactId>org.osgi.service.metatype.annotations</artifactId>
                <version>${osgi-ds-version}</version>
                <scope>provided</scope>
            </dependency>

            <dependency>
                <groupId>com.google.guava</groupId>
